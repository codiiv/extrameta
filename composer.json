{
    "name": "codiiv/extrameta",
    "description": "Added Meta tables to laravel. Inspired by the Wordpress database table structure",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.7.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Janvier M",
            "email": "codiivdev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Codiiv\\Extrameta\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codiiv\\Extrameta\\ExtrametaServiceProvider"
            ]
        }
    }
